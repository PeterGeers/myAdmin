            application/json:
              schema:
                type: object
                properties:
                  success:
                    type: boolean
                  modules:
                    type: object
                    properties:
                      Finance:
                        type: boolean
                      STR:
                        type: boolean
    post:
      tags:
        - Tenant Module Management
      summary: Update tenant module access
      security:
        - BearerAuth: []
        - TenantHeader: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: object
              required:
